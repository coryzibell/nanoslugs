{
    "name": "coryzibell/nanoslugs",
    "description": "Hashes the Id of an entry when it is saved and replaces the slug.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "nanoslugs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cory Zibell",
            "homepage": "https://coryzibell.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "hashids/hashids": "^2.0"
    },
    "autoload": {
        "psr-4": {
          "coryzibell\\nanoslugs\\": "src/"
        }
    },
    "extra": {
        "name": "Nanoslugs",
        "handle": "nanoslugs",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/coryzibell/nanoslugs/master/CHANGELOG.md",
        "components": {
            "nanoslugsService": "coryzibell\\nanoslugs\\services\\NanoslugsService"
        },
        "class": "coryzibell\\nanoslugs\\Nanoslugs"
    }
}
